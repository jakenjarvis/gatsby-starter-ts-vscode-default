{
  "name": "gatsby-starter-ts-vscode-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby using TypeScript on vscode.",
  "version": "0.1.0",
  "author": "Jaken <jaken.jarvis@gmail.com>",
  "dependencies": {
    "eslint-config-react-app": "^5.2.1",
    "gatsby": "^2.24.63",
    "gatsby-image": "^2.4.19",
    "gatsby-plugin-manifest": "^2.4.30",
    "gatsby-plugin-offline": "^3.2.28",
    "gatsby-plugin-react-helmet": "^3.3.11",
    "gatsby-plugin-sharp": "^2.6.36",
    "gatsby-source-filesystem": "^2.3.30",
    "gatsby-transformer-sharp": "^2.5.15",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@danbruegge/gatsby-plugin-stylelint": "^4.1.0",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "eslint": "6.x",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "prettier": "^2.1.2",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended": "^3.0.0",
    "typescript": "^4.0.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "typecheck": "tsc -p . --noEmit",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,json,md}\" --ignore-path .eslintignore",
    "lintfix": "eslint \"src/**/*.{js,jsx,ts,tsx,json,md}\" --ignore-path .eslintignore --fix",
    "stylelint": "stylelint \"src/**/*.{js,jsx,ts,tsx,css,less,sass,scss}\"",
    "stylelintfix": "stylelint \"src/**/*.{js,jsx,ts,tsx,css,less,sass,scss}\" --fix",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jakenjarvis/gatsby-starter-ts-vscode-default"
  },
  "bugs": {
    "url": "https://github.com/jakenjarvis/gatsby-starter-ts-vscode-default/issues"
  }
}
